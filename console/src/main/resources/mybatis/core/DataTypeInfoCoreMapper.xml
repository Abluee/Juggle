<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.somta.juggle.console.infrastructure.mapper.DataTypeInfoMapper" >
<!-- Result Map-->
<resultMap id="BaseResultMap" type="net.somta.juggle.console.infrastructure.po.DataTypeInfoPO" >
	<result column="id" property="id"/>
	<result column="data_type_classify" property="dataTypeClassify"/>
	<result column="data_type" property="dataType"/>
	<result column="display_name" property="displayName"/>
	<result column="object_key" property="objectKey"/>
	<result column="object_structure" property="objectStructure"/>
	<result column="created_at" property="createdAt"/>
	<result column="created_by" property="createdBy"/>
	<result column="updated_at" property="updatedAt"/>
	<result column="updated_by" property="updatedBy"/>
</resultMap>

<sql id="Example_Where_Clause">
	where 1=1
</sql>
       
<!-- t_data_type_info table all fields -->
<sql id="Base_Column_List" >
	id, data_type_classify, data_type, display_name, object_key, object_structure, created_at, created_by, updated_at, updated_by
</sql>
   
<insert id="add" parameterType="net.somta.juggle.console.infrastructure.po.DataTypeInfoPO" >
	insert into t_data_type_info (id, data_type_classify, data_type, display_name, object_key, object_structure)
	values (#{id},#{dataTypeClassify},#{dataType},#{displayName},#{objectKey},#{objectStructure});
</insert>

<delete id="deleteById" parameterType="java.lang.Long">
 delete from t_data_type_info where id=#{id}
</delete>

<update id="update" parameterType="net.somta.juggle.console.infrastructure.po.DataTypeInfoPO" >
	update t_data_type_info set
	<trim  suffixOverrides="," >
	<if test="data_type_classify != null">
		data_type_classify=#{dataTypeClassify},
	</if>
	<if test="dataType != null">
		data_type=#{dataType},
	</if>
	<if test="displayName != null and displayName != ''">
		display_name=#{displayName},
	</if>
	<if test="objectKey != null and objectKey != ''">
		object_key=#{objectKey},
	</if>
	<if test="objectStructure != null and objectStructure != ''">
		object_structure=#{objectStructure},
	</if>
	<if test="createdAt != null">
		created_at=#{createdAt},
	</if>
	<if test="createdBy != null">
		created_by=#{createdBy},
	</if>
	<if test="updatedAt != null">
		updated_at=#{updatedAt},
	</if>
	<if test="updatedBy != null">
		updated_by=#{updatedBy},
	</if>
	</trim> where id=#{id}
</update>

<select id="queryById" resultMap="BaseResultMap" parameterType="java.lang.Long">
	select <include refid="Base_Column_List" /> 
	from t_data_type_info where id = #{id}
</select>

<select id="queryListCount" resultType="java.lang.Long" parameterType="net.somta.juggle.console.domain.datatype.vo.DataTypeInfoQueryVo">
	select count(*)
	from t_data_type_info
	<include refid="Example_Where_Clause"/>
</select>

<select id="queryByList" resultMap="BaseResultMap" parameterType="net.somta.juggle.console.domain.datatype.vo.DataTypeInfoQueryVo">
	select <include refid="Base_Column_List" /> 
	from t_data_type_info
</select>

</mapper>   
